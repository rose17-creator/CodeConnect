{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2016",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true /* Skip type checking all .d.ts files. */
  }
}
